{
  "name": "lacona-lacolor",
  "version": "0.0.1",
  "description": "Lacona Addon to Lacolor is a color chooser and convertor for Lacona",
  "main": "lib/index.js",
  "lacona": {
    "title": "Lacolor",
    "description": "Lacolor is a color chooser and convertor for Lacona",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdaitzman/lacolor"
  },
  "dependencies": {
    "lacona-phrases": "^1.0.0",
    "lacona-api": "^1.0.0",
    "elliptical": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-cli": "^6.0.0",
    "rimraf": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
